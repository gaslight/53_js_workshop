var data = [/* 0 .. 100 */];
for (var i = 0; i < 100; i++) {
  data.push(i);
}

function sumEqualProduct(element, index, array) {
  var parts = (element + "").split("", 2);
  var first = parseInt(parts[0], 10);
  var second = parseInt(parts[1], 10);
  return first+second == first*second;
}

// Implement the some function so that it indicates if we have at least one
// result from the function that is true.

function some(array, func) {

}

console.log(some(data, sumEqualProduct));

